  "version": "0.2.0",
  "configurations": [
  
    {
      "command": "npm run compare",
      "name": "Compare Algo",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "npm run wrostDFA",
      "name": "Wrost DFA",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "node dist/test_nodejs/Test_wrost_RFSA.js",
      "name": "Wrost RFSA",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "npm run lstar",
      "name": "Test L-Star",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "npm run nlstar",
      "name": "Test NL-Star",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "node dist/test_nodejs/Automaton_minimize_test.js",
      "name": "Test Minimize",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome index hidden",
      "url": "http://localhost:5500/index.html",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js",
        "!**/node_modules/**"
      ]
    }
  ]
}